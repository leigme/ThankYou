<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_school_announcement_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yhcloud.thankyou.module.schoolannouncement.view.SchoolAnnouncementDetailActivity">
    <include layout="@layout/include_header"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp">
        <com.yhcloud.thankyou.utils.myview.MyWebView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/mwv_schoolannouncement_content">
        </com.yhcloud.thankyou.utils.myview.MyWebView>
    </RelativeLayout>
</LinearLayout>
